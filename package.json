{
  "name": "replicache",
  "description": "Realtime sync for any backend stack",
  "version": "10.0.0-alpha.2",
  "repository": "github:rocicorp/replicache",
  "license": "BSL-1.1",
  "scripts": {
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch",
    "format": "prettier --write '{doc,src,sample,perf}/**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check '{doc,src,sample,perf}/**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-types": "tsc --noEmit && tsc --noEmit tool/cli.ts",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/ perf/",
    "build-cli": "esbuild --bundle --platform=node --target=es2018 --external:'node:*' --outfile=out/cli.cjs --format=cjs tool/cli.ts",
    "build-dts": "rm -rf out/.dts/ && tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup.config.js && rm -rf out/.dts",
    "build-mjs": "esbuild --bundle --target=es2018 --outfile=out/replicache.mjs --format=esm src/mod.ts",
    "build-cjs": "esbuild --bundle --target=es2018 --outfile=out/replicache.js --format=cjs src/mod.ts",
    "build-min-mjs": "esbuild --bundle --target=es2018 --outfile=out/replicache.min.mjs --format=esm src/mod.ts --minify --mangle-props=^_",
    "build": "rm -rf out && npm run build-dts && npm run build-mjs && npm run build-cjs && npm run build-min-mjs && npm run build-cli",
    "prepack": "npm run lint && npm run test && npm run build",
    "prepare": "rm -f node_modules/fetch-mock/esm/client.d.ts",
    "perf": "node perf/runner.js"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@rocicorp/licensing": "^4.0.0-beta.3",
    "@rocicorp/lock": "^1.0.1",
    "@rocicorp/logger": "^2.2.0",
    "@rocicorp/resolver": "^1.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@web/dev-server": "0.1.29",
    "@web/dev-server-esbuild": "^0.3.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "benchmark": "^2.1.4",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.2",
    "esbuild": "^0.14.34",
    "eslint": "^8.12.0",
    "fetch-mock": "^9.11.0",
    "get-port": "^6.1.2",
    "hash-wasm": "^4.9.0",
    "idb": "^7.0.1",
    "navigator.locks": "^0.8.5",
    "playwright": "^1.20.2",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "sinon": "^13.0.1",
    "typescript": "^4.6.3",
    "xbytes": "^1.7.0"
  },
  "main": "out/replicache.js",
  "module": "out/replicache.mjs",
  "types": "out/replicache.d.ts",
  "bin": {
    "replicache": "./out/cli.cjs"
  },
  "exports": {
    "module": "./out/replicache.mjs",
    "require": "./out/replicache.js",
    "default": "./out/replicache.mjs"
  },
  "files": [
    "out/cli.mjs",
    "out/replicache.d.ts",
    "out/replicache.js",
    "out/replicache.mjs",
    "out/replicache.wasm",
    "out/replicache.dev.wasm"
  ]
}
